<query id="updateAdminVersion" action="update">
	<tables>
		<table name="svmarket_version" />
	</tables>
	<columns>
		<column name="version" var="version" />
        <column name="og_description" var="og_description" />
		<column name="description" var="description" />
	</columns>
	<conditions>
		<condition operation="equal" column="version_srl" var="version_srl" notnull="notnull" />
	</conditions>
</query>