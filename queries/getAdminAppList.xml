<query id="getAdminAppList" action="select">
	<tables>
		<table name="svmarket_app" />
	</tables>
	<columns>
		<column name="app_srl" />
	</columns>
	<conditions>
		<condition operation="equal" column="package_srl" var="package_srl" />
        <condition operation="equal" column="display" var="display" pipe="and"/>
	</conditions>
</query>