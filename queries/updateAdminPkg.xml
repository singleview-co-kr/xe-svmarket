<query id="updateAdminPkg" action="update">
	<tables>
		<table name="svmarket_package" />
	</tables>
	<columns>
		<column name="module_srl" var="module_srl" />
		<column name="title" var="title" />
        <column name="og_description" var="og_description" />
		<column name="description" var="description" />
        <column name="homepage" var="homepage" />
        <column name="tags" var="tags" />
		<column name="display" var="display" />
		<column name="list_order" var="list_order" />
		<column name="thumb_file_srl" var="thumb_file_srl" />
	</columns>
	<conditions>
		<condition operation="equal" column="package_srl" var="package_srl" notnull="notnull" />
	</conditions>
</query>