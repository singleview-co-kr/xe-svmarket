<query id="getAdminAppDetail" action="select">
	<tables>
		<table name="svmarket_app" />
	</tables>
	<columns>
		<column name="*" />
	</columns>
	<conditions>
		<condition operation="equal" column="app_srl" var="app_srl" />
        <condition operation="equal" column="display" var="display" pipe="and"/>
	</conditions>
</query>