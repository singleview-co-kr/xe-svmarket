<query id="updateAppReadedCount" action="update">
    <tables>
        <table name="svmarket_app" />
    </tables>
    <columns>
        <column name="readed_count" var="readed_count" default="plus(1)" />
    </columns>
    <conditions>
        <condition operation="equal" column="app_srl" var="app_srl" filter="number" notnull="notnull" />
    </conditions>
</query>
