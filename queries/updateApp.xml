<query id="updateApp" action="update">
	<tables>
		<table name="svmarket_apps" />
	</tables>
	<columns>
		<column name="module_srl" var="module_srl" />
		<column name="title" var="title" />
		<column name="description" var="description" />
        <column name="github_url" var="github_url" />
        <column name="homepage" var="homepage" />
        <column name="pkg_tags" var="pkg_tags" />
		<column name="display" var="display" />
		<column name="list_order" var="list_order" />
		<column name="thumb_file_srl" var="thumb_file_srl" />
        <column name="updatetime" var="updatetime" />
	</columns>
	<conditions>
		<condition operation="equal" column="pkg_srl" var="pkg_srl" notnull="notnull" />
	</conditions>
</query>